@using Microsoft.CST.OAT.Utils;
@using Serilog;
@using System.Reflection

<DictionaryObjectObject id="@id" Object="@Object" SubPath="@SubPath" dictionaryKeyType="@typeof(string)" dictionaryValueType="@dictionaryValueType" buttonText="@buttonText" onChangeAction="onChangeAction" />

@code 
{
    [Parameter]
    public object? Object { get; set; }

    [Parameter]
    public string? SubPath { get; set; }

    [Parameter]
    public string buttonText { get; set; } = "Dictionary Key And Value";

    [Parameter]
    public string id { get; set; } = string.Empty;

    [Parameter]
    public Action onChangeAction { get; set; } = () => { };

    [Parameter]
    public Type? dictionaryValueType { get; set; }
}